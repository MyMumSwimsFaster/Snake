<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/santicell"
        android:layout_width="169dp"
        android:layout_height="42dp"
        android:text="SANTICELL"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="25dp" />

    <Button
        android:id="@+id/credits"
        android:layout_width="92dp"
        android:layout_height="36dp"
        android:text="credits"
        app:layout_constraintEnd_toStartOf="@+id/scam"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="668dp" />

    <Button
        android:id="@+id/scam"
        android:layout_width="45dp"
        android:layout_height="40dp"
        android:text="+"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/credits"
        tools:layout_editor_absoluteY="668dp" />

    <TextView
        android:id="@+id/snekk"
        android:layout_width="91dp"
        android:layout_height="41dp"
        android:layout_marginTop="4dp"
        android:text="Snekk"
        android:textIsSelectable="false"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/santicell" />

</androidx.constraintlayout.widget.ConstraintLayout>